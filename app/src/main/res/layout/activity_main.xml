<?xml version="1.0" encoding="utf-8"?>
<Linearlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/votes"
        android:entries="@array/Votting"/>
    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/voteCount"
        android:entries="@array/Count"/>
    <Button
        android:id="@+id/chooseme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CHOOSE"
        android:onClick="CHOOSE"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Answer"
        android:textSize="20dp"
        android:layout_marginTop="10dp"/>

</Linearlayout.widget.ConstraintLayout>